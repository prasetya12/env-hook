{
  "name": "env-hook",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.4.0",
    "env-cmd": "^10.1.0",
    "prop-types": "^15.8.1",
    "props-type": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start:local": "env-cmd -f ./.local.env react-scripts start",
    "build:local": "env-cmd -f ./.local.env react-scripts build",
    "start:staging": "env-cmd -f ./.staging.env react-scripts start",
    "build:staging": "env-cmd -f ./.staging.env react-scripts build",
    "start:production": "env-cmd -f ./.production.env react-scripts start",
    "build:production": "env-cmd -f ./.production.env react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
